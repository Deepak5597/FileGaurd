--Users table
create table fg_users(email varchar(1000),password varchar(1000),fname varchar(250),lname varchar(200));


-- insert query

insert into fg_users values(?,?,?,?);

----file name
create table fg_files(fileid INT AUTO_INCREMENT PRIMARY KEY,filename VARCHAR(250),filepath varchar(1000),isDirectory VARCHAR(100),directoryname VARCHAR(250),filetype  VARCHAR(250),encryptiontype  VARCHAR(250),creationdate DATETIME,createdby VARCHAR(250),filesize VARCHAR(250),systemfilename VARCHAR(250));



create table fg_all_files(fileid INT AUTO_INCREMENT PRIMARY KEY,filename VARCHAR(250),isDirectory VARCHAR(100),directoryname VARCHAR(250),filetype  VARCHAR(250),encryptiontype  VARCHAR(250),creationdate DATETIME,createdby VARCHAR(250),filesize VARCHAR(250),file mediumblob);



insert into fg_files(filename,isDirectory,directoryname,filetype,encryptiontype,createdby,creationdate,filesize,file) values(?,?,?,?,?,?,CURDATE(),?,?)"



